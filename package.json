{
  "name": "matryoshka-js",
  "version": "0.0.0",
  "description": "lang to lang to lang to lang..",
  "preferGlobal": "true",
  "bin": {
    "matryoshka-js": "index.js"
  },
  "scripts": {
    "test": "mocha",
    "cover" : "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover-view" : "istanbul cover ./node_modules/mocha/bin/_mocha && open ./coverage/lcov-report/index.html",
    "cover-clean" : "rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodar-chkuaselidze/matryoshka.git"
  },
  "keywords": [
    "convert",
    "language",
    "cli"
  ],
  "author": "Nodari Chkuaselidze",
  "license": "The Unlicense",
  "bugs": {
    "url": "https://github.com/nodar-chkuaselidze/matryoshka/issues"
  },
  "homepage": "https://github.com/nodar-chkuaselidze/matryoshka",
  "dependencies": {
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "lodash": "^2.4.1",
    "mocha-lcov-reporter": "0.0.1",
    "optimist": "^0.6.1",
    "q": "^1.0.1",
    "q-io": "^1.11.4"
  },
  "devDependencies": {
    "mocha": "^1.21.4",
    "should": "^4.0.4"
  }
}
